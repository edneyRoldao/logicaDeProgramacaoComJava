package workingWithDates;

import java.util.Calendar;
import java.util.GregorianCalendar;

/**
 * 
 * @author edneyroldao
 *
 *	A classe calendar e abstrata, portando nao podemos instancia-la
 *
 */
public class FirstExampleClassCalendar {

	public static void main(String[] args) {
		
		Calendar a = Calendar.getInstance(); // aqui e retornado um gregorian calendar
		Calendar b = new GregorianCalendar(); // e a mesmo coisa
		
		b.set(1982, Calendar.MAY, 10);

		a.getTime(); // return um objeto do tipo calendar
		
	}
	
	
}
